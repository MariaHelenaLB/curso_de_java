package meu_primeiro_projeto;

public class DonoCarro {

	public static void main(String[] args) {
		Proprietario dono = new Proprietario();
		dono.nome = "Maria Helena Lima Barreto";
		dono.cpf = "000.000.000.-00";
		dono.idade = 25;
		dono.bairro = "Rua xxxxx xxxxx, 353";
		dono.cidade = "Guarulhos-SP";
		
		System.out.println("Nome: " + dono.nome );
		System.out.println("CPF: " + dono.cpf);
		System.out.println("Idade: " + dono.idade);
		System.out.println("Bairro: " + dono.bairro);
		System.out.println("Cidade: " + dono.cidade);
		
		Carro seuCarro= new Carro();
		seuCarro.marca = "Audi";
		seuCarro.modelo = "RS7";
		seuCarro.fabricante = "Volkswagen";
		seuCarro.anoDeFabricacao = 2024;
		seuCarro.cor = "Roxo";
		
		System.out.println("Marca: " + seuCarro.marca);
		System.out.println("Modelo: " + seuCarro.modelo);
		System.out.println("Fabricante: " +  seuCarro.fabricante);
		System.out.println("Ano de fabricação: " + seuCarro.anoDeFabricacao);
		System.out.println("Cor: " + seuCarro.cor);

	}

}
